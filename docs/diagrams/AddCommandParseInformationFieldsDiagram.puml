@startuml
!include style.puml

skinparam defaultFontSize 15
skinparam titleFontSize 15

title Parse Information Fields

box Logic LOGIC_COLOR_T1
participant ":AddCommandParser" as AddCommandParser LOGIC_COLOR
participant "<<class>>\nParserUtil" as ParserUtil LOGIC_COLOR
end box

AddCommandParser->ParserUtil:parseName(name)
activate AddCommandParser
activate ParserUtil
return name
AddCommandParser->ParserUtil:parseTags(tags)
activate ParserUtil
return tags
AddCommandParser->ParserUtil:parseModules(modules)
activate ParserUtil
return modules
opt ifPrefixIsPresent
    AddCommandParser->ParserUtil:parsePhone(phone)
    activate ParserUtil
    return phone
    AddCommandParser->ParserUtil:parseEmail(email)
    activate ParserUtil
    return email
    AddCommandParser->ParserUtil:parseAddress(address)
    activate ParserUtil
    return address
    AddCommandParser->ParserUtil:parseBirthday(birthday)
    activate ParserUtil
    return birthday
end
AddCommandParser->ParserUtil:parseSocialMedia(socialMedia)
activate ParserUtil
return socialMedia

@enduml